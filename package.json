{
  "name": "@elizaos/plugin-aithra-toolkit",
  "version": "1.0.0",
  "main": "dist/index.js",
  "type": "module",
  "types": "dist/index.d.ts",
  "agentConfig": {
    "pluginType": "elizaos:plugin:1.0.0",
    "pluginParameters": {
      "SOLANA_RPC_URL": {
        "type": "string",
        "description": "Solana RPC URL"
      },
      "AITHRA_PRIVATE_KEY": {
        "type": "string",
        "description": "Aithra Private Key"
      },
      "AITHRA_PRIORITY_FEE": {
        "type": "number",
        "description": "Aithra Priority Fee"
      }
    }
  },
  "dependencies": {
    "@elizaos/core": "workspace:*",
    "@aithranetwork/sdk-aithra-toolkit": "^0.0.3",
    "@solana/web3.js": "^1.95.8",
    "bs58": "^6.0.0",
    "zod": "^3.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "tsup": "8.3.5"
  },
  "scripts": {
    "prebuild": "node -e \"const pkg=require('./package.json');pkg.dependencies['@elizaos/core']='workspace:*';require('fs').writeFileSync('package.json',JSON.stringify(pkg,null,2))\"",
    "build": "tsup --format esm --dts",
    "dev": "tsup --format esm --dts --watch",
    "lint": "eslint --fix  --cache ."
  }
}